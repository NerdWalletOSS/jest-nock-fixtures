{
  "name": "@nerdwallet/jest-nock-fixtures",
  "description": "jest-nock-fixtures",
  "author": "benjroy <ballen@nerdwallet.com>",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "nw-app-build babel src --out-dir dist --copy-files",
    "prebuild": "npm run clean",
    "build-watch": "npm run build -- --watch",
    "prebuild-watch": "npm run clean",
    "mocha": "BABEL_ENV=test mocha --recursive test",
    "fix-lint": "npm run lint -- --fix",
    "lint": "eslint . --ext .js --ext .jsx",
    "test": "nyc npm run mocha && mv ./coverage/cobertura-coverage.xml ./build/metrics/coverage.xml"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NerdWallet/nwjs-jest-nock-fixtures.git"
  },
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@nerdwallet/eslint-config": "^2.0.1",
    "@nerdwallet/nw-app-build": "^5.11.0",
    "babel-eslint": "^8.0.0",
    "chai": "^4.1.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^5.2.0",
    "nyc": "^10.2.0",
    "proxyquire": "^1.7.11"
  }
}
